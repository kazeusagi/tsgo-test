{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "stream",
  "tasks": {
    "tsc": {
      "dependsOn": ["^tsc"],
      "cache": false
    },
    "tsgo": {
      "dependsOn": ["^tsgo"],
      "persistent": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
